{
  "name": "fetch-mock",
  "version": "11.0.0",
  "description": "Mock http requests made using fetch (or isomorphic-fetch)",
  "main": "./dist/commonjs.cjs",
  "module": "./src/index.js",
  "exports": {
    "browser": "./src/index.js",
    "import": {
      "default": "./src/index.js",
      "types": "./types/index.d.ts"
    },
