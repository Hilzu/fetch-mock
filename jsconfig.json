{
    "compilerOptions": {
        "lib": [
            "es6",
            "dom"
        ],
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": false,
        "strictFunctionTypes": true,
        "types": ["packages/core/types/index.d.ts"],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "packages/core/src/Route.js"
        "packages/core/types/index.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
