{
    "compilerOptions": {
        "lib": [
            "es6",
            "dom"
        ],
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": false,
        "strictFunctionTypes": true,
        "types": [
            "packages/core/types/InstanceManagement.d.ts"
        ],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "packages/core/src/InstanceManagement.js",
        "packages/core/types/InstanceManagement.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}